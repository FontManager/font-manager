<?xml version="1.0" encoding="UTF-8"?>
<interface domain="font-manager">
  <requires lib="gtk" version="4.12"/>
  <template class="FontManagerOrthographyListRow" parent="GtkGrid">
    <property name="margin-start">12</property>
    <property name="margin-end">12</property>
    <property name="margin-top">2</property>
    <property name="margin-bottom">2</property>
    <child>
      <object class="GtkLabel" id="C_name">
        <property name="opacity">0.65</property>
        <property name="margin-start">6</property>
        <property name="margin-end">6</property>
        <property name="margin-top">6</property>
        <property name="margin-bottom">6</property>
        <property name="hexpand">1</property>
        <property name="vexpand">0</property>
        <layout>
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel" id="native_name">
        <property name="margin-start">8</property>
        <property name="margin-end">8</property>
        <property name="margin-top">8</property>
        <property name="margin-bottom">8</property>
        <property name="hexpand">1</property>
        <property name="vexpand">0</property>
        <property name="ellipsize">3</property>
        <attributes>
          <attribute name="scale" value="1.2"/>
        </attributes>
        <layout>
          <property name="column">0</property>
          <property name="row">1</property>
          <property name="row-span">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLevelBar" id="coverage">
        <property name="margin-start">6</property>
        <property name="margin-end">6</property>
        <property name="margin-top">6</property>
        <property name="margin-bottom">6</property>
        <property name="hexpand">1</property>
        <property name="vexpand">0</property>
        <layout>
          <property name="column">0</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
  </template>
</interface>
