<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="FontManagerFontListBoxRow" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkBox">
        <property name="valign">start</property>
        <child>
          <object class="GtkCheckButton" id="item_state">
            <property name="active">True</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="item_name">
            <property name="ellipsize">end</property>
            <property name="halign">start</property>
            <property name="hexpand">True</property>
            <property name="margin-end">9</property>
            <property name="margin-start">9</property>
            <property name="valign">center</property>
            <property name="xalign">0.0</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="item_preview">
            <property name="ellipsize">middle</property>
            <property name="halign">start</property>
            <property name="hexpand">True</property>
            <property name="margin-start">6</property>
            <property name="valign">center</property>
            <property name="xalign">0.0</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="item_count">
            <style><class name="count"/></style>
            <property name="halign">center</property>
            <property name="margin-end">6</property>
            <property name="margin-start">6</property>
            <property name="valign">center</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>

