default: all

fontutils:
	$(BUILD_LIB)
	$(STRIP_LIB)

pot:
	@cd po && $(SH) ./pogen.sh && cd ..

compile_po: pot
	for p in `ls po|grep .po|grep -v $(PACKAGE_NAME)`; \
	do \
		lang=${p%.po}; \
		langdir=$$lang/LC_MESSAGES; \
		$(INSTALL_DIR) po/$$langdir; \
		msgmerge -o - po/$$p po/$(PACKAGE_NAME).pot | \
		msgfmt -c -o po/$$langdir/$(PACKAGE_NAME).mo -; \
	done

install_mo:
	for mo in `$(FIND) po -name $(PACKAGE_NAME).mo`; \
	do \
		$(INSTALL_DIR) \
		`echo $$mo | sed "s|^po|$(INST_LOC_DIR)|" | xargs dirname`; \
		$(INSTALL_FILE) $$mo `echo $$mo | sed "s|^po|$(INST_LOC_DIR)|"` ; \
	done

@NLS_TRUE@  all: fontutils $(SRC_DIR)/constants.py $(SRC_DIR)/$(PACKAGE_NAME) \
@NLS_TRUE@  $(SRC_DIR)/$(SAMPLER) $(SRC_DIR)/main.py $(SRC_UI_DIR)/export.py \
@NLS_TRUE@  $(SRC_CORE_DIR)/__init__.py compile_po
@NLS_FALSE@  all: fontutils $(SRC_DIR)/constants.py $(SRC_DIR)/$(PACKAGE_NAME) \
@NLS_FALSE@  $(SRC_DIR)/$(SAMPLER) $(SRC_DIR)/main.py $(SRC_UI_DIR)/export.py \
@NLS_FALSE@  $(SRC_CORE_DIR)/__init__.py
	$(Py_Compile)
	$(Py_Optimize)

